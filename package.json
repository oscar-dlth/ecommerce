{
  "name": "stories-api",
  "version": "1.0.0",
  "description": "stories backend using clean arquitecture",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc && tsc-alias --watch\" \"nodemon -q dist/index.js\"",
    "test": "npm run build && npx jest --detectOpenHandles --silent",
    "migrate": "npx sequelize-cli db:migrate",
    "seed:all": "npx sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscar-dlth/stories-api.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oscar-dlth/stories-api/issues"
  },
  "homepage": "https://github.com/oscar-dlth/stories-api#readme",
  "dependencies": {
    "brandi": "^4.0.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.3.0",
    "mediatr-ts": "^1.2.0",
    "mysql2": "^3.1.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "tsc-alias": "^1.8.6",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.0",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.2.2",
    "jest": "^28.1.2",
    "mongoose": "^5.3.7",
    "nodemon": "^2.0.18",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5"
  }
}
